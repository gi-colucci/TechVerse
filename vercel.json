{
    "version": 2,
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "/frontend/index.html"
      }
    ],
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/node"
      }
    ],
    
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/frontend/index.html"
      }
    ]
    
  }